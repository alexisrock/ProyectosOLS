
@{
	ViewData["Title"] = "Proyectos";
}

<h1>Proyectos</h1>




<div class="row">


	<table class="table table-bordered table-striped" id="tblCliente">
		<thead>
			<tr>
				<th>Nombre proyeecto</th>
				<th>Clilente</th>
				<th>Estado</th>
				<th>Fecha inicio</th>
				<th>Fecha final</th>
				<th>Precio</th>
				<th>Cantidad de horas</th>
				<th>Accion</th>
			</tr>

		</thead>
		<tbody>
			@foreach (var item in (List<Proyectos>)ViewBag.proyectos)
			{
			<tr>
				<td>@item.nombre </td>
				<td>@item.cliente </td>
				<td>@item.estado </td>
				<td>@item.fechainicio </td>
				<td>@item.fechafin </td>
				<td>@item.precio </td>
				<td>@item.cantidadhoras </td>
				<td><a href="@Url.Action("Index","Clientes")" class="btn btn-outline-dark">Ver</a></td>




			</tr>


			}



		</tbody>

	</table>

</div>


